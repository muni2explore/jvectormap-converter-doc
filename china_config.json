[{
    "name": "read_data",
    "file_name": "cn_countries/cn_countries.shp",
    "filter": "continent != 'Antarctica' AND continent != 'Seven seas (open ocean)' ",
    "longitude0": 11.5,
    "projection": "mill"
}, {
    "name": "write_data",
    "format": "jvectormap",
    "file_name": "./output/jquery.vmap.china.js",
    "params": {
        "code_field": "iso_a2",
        "name_field": "name"
    }
}]